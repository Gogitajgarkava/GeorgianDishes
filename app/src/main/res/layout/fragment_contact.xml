<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainPage.MenuFragment.FragmentContact">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="+995 032 25 60 60"
            android:layout_margin="25dp"
            android:drawableLeft="@drawable/contact"
            android:drawablePadding="45dp"/>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Info@jgarkavasindustries.ge"
            android:textSize="15sp"
            android:drawablePadding="45dp"
            android:drawableLeft="@drawable/mail"
            android:layout_marginLeft="25dp"/>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Jgarkavasindustries.ge"
            android:layout_margin="25dp"
            android:textSize="15sp"
            android:drawablePadding="45dp"
            android:drawableLeft="@drawable/mail"/>
    </LinearLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact"
        android:textSize="50sp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="200dp"/>
</FrameLayout>